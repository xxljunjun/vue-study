    "vue": "^2.6.11",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^9.1.2",
    "vue-json-excel": "^0.3.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1",
    "wangeditor": "^4.7.1",
    "xxl-lang": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
